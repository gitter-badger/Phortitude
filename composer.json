{
    "name": "phortitude/phortitude",
    "description": "A collection of components for PHP developers",
    "keywords": ["library", "component"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Kraus",
            "email": "sebastian.kraus@dualforge.com",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://github.com/phortitude/phortitude/issues",
        "source": "https://github.com/phortitude/phortitude"
    },
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "~0.7",
        "squizlabs/php_codesniffer": "~2.3",
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Phortitude\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phortitude\\Test\\": "tests",
            "Phortitude\\Test\\Regression\\": "tests/regression",
            "Phortitude\\Test\\Fixture\\": "tests/fixtures"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        }
    }
}
